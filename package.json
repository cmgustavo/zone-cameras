{
  "name": "zone-cameras",
  "version": "0.0.1",
  "author": "Gustavo Cortez",
  "homepage": "https://github.com/cmgustavo/zone-cameras/",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "electron": "electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean-all": "git clean -dfx",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "build:prepare:ios": "npx cap add ios",
    "build:prepare:android": "npx cap add android",
    "build:resources:ios": "cordova-res ios --skip-config --copy",
    "build:resources:android": "cordova-res android --skip-config --copy",
    "build:prod": "ionic build --prod",
    "build:ios": "ionic capacitor copy ios",
    "build:android": "ionic capacitor copy android",
    "final:ios": "npm run build:prod && npm run build:prepare:ios && npm run build:resources:ios && npm run open:ios",
    "final:android": "npm run build:prod && npm run build:prepare:android && npm run build:resources:android && npm run open:android"
  },
  "dependencies": {
    "@angular/common": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@capacitor/android": "^2.4.3",
    "@capacitor/core": "2.4.3",
    "@capacitor/ios": "^2.4.3",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
    "@ionic/angular": "^5.5.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@angular/language-service": "~11.0.2",
    "@capacitor/cli": "2.4.3",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.9",
    "codelyzer": "^6.0.0",
    "electron": "^11.4.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  },
  "description": "A front view for Zone Minder API",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
